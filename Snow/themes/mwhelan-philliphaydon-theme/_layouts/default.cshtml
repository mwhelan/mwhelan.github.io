@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<Snow.ViewModels.BaseViewModel>
@using System.Collections.Generic
@{
    var keywords = String.Join(",", Model.Keywords);
}
<!DOCTYPE html>
<html dir="ltr" lang="en-US">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="last-modified" content="@Model.GeneratedDate" />
    <meta name="keywords" content="@keywords" />
    <title>michael whelan</title>
    <link rel="stylesheet" type="text/css" href="/stylesheets/style.css" />
    @Html.CanonicalUrl()
</head>
<body class="home blog">
    <div id="page" class="hfeed site">
        <header id="masthead" class="site-header fancy-container lightgreen" role="banner">
            <hgroup>
                <h1 class="site-title"><a href="/" title="michael whelan" rel="home">michael whelan</a></h1>
                @*<h2><small>blog.Should().Be(behaviour_driven);</small></h2>*@
                <p class="site-description">Blog about development and testing in .Net.</p>
            </hgroup>
            <nav role="navigation" class="site-navigation main-navigation">
                <h1 class="assistive-text">Menu</h1>
                <div class="assistive-text skip-link"><a href="#content" title="Skip to content">Skip to content</a></div>
                <div class="menu">
                    <ul>
                        <li><a href="http://about.me/michaelwhelan">About Me</a></li>
                        <li><a href="/category/">Categories</a></li>
                        <li><a href="/archive/">Archive</a></li>
                        <li><a href="/rss.xml">RSS</a> / <a href="/feed.xml">Atom</a></li>
                    </ul>
                </div>
            </nav>
        </header>
        <div id="main">
            <div id="primary" class="site-content">
                <div id="content" role="main">
                  @RenderBody()
                </div>
                <!-- #content -->
            </div>
            <!-- #primary .site-content -->

            <div id="secondary" class="widget-area fancy-container lightred" role="complementary">
                <aside id="text-5" class="widget widget_text">
                    <h1 class="widget-title">Follow Me</h1>
                    <div class="textwidget logos">
                        <a href="https://twitter.com/mjmwhelan" onclick="javascript:_gaq.push(['_trackEvent','outbound-widget','http://twitter.com']);"><img src="/stylesheets/images/twitter.png"></a>
                        <a href="https://github.com/mwhelan" onclick="javascript:_gaq.push(['_trackEvent','outbound-widget','http://github.com']);"><img src="/stylesheets/images/github.png"></a>
                        <a href="http://www.linkedin.com/in/mwhelan" onclick="javascript:_gaq.push(['_trackEvent','outbound-widget','http://www.linkedin.com']);"><img src="/stylesheets/images/linkedin.png"></a>
                        <a href="https://plus.google.com/u/1/105055485720815974241?rel=me" onclick="javascript:_gaq.push(['_trackEvent','outbound-widget','http://plus.google.com']);"><img src="/stylesheets/images/googleplus.png"></a>
                    </div>
                    
                    <a href="http://teststack.net"><img src="https://avatars2.githubusercontent.com/u/1782385?s=280" width="208" height="208"></a>
                    
                </aside>

                <aside id="linkcat-6" class="widget widget_links">
                    <h1 class="widget-title">Blog Roll</h1>
                    <ul class='xoxo blogroll'>
                        <li><a href="http://birgitwhelan.com/" target="_top" onclick="javascript: _gaq.push(['_trackEvent', 'outbound-blogroll', 'http://birgitwhelan.com/']);">Birgit Whelan</a></li>
                        <li><a href="http://www.mehdi-khalili.com/" target="_top" onclick="javascript: _gaq.push(['_trackEvent', 'outbound-blogroll', 'http://www.mehdi-khalili.com/']);">Mehdi Khalili</a></li>
                        <li><a href="http://robdmoore.id.au/" target="_top" onclick="javascript: _gaq.push(['_trackEvent', 'outbound-blogroll', 'http://robdmoore.id.au/']);">Rob Moore</a></li>
                        <li><a href="http://jake.ginnivan.net/" target="" onclick="javascript: _gaq.push(['_trackEvent', 'outbound-blogroll', 'http://jake.ginnivan.net/']);">Jake Ginnivan</a></li>
                        <li><a href="http://kozmic.net/" target="" onclick="javascript: _gaq.push(['_trackEvent', 'outbound-blogroll', 'http://kozmic.net/']);">Krzysztof Kozmic</a></li>
                    </ul>
                    
                </aside>
                <aside id="archives-4" class="widget widget_archive">
                    <h1 class="widget-title">Archive</h1>
                    <ul class="posts">
                    @foreach(var item in Model.MonthYearList)
                    {
                          <li><a href="@item.Url">@item.Title</a></li>
                    }
                    </ul>
                </aside>
            </div>
            <!-- #secondary .widget-area -->


        </div>
        <!-- #main -->

        <footer id="colophon" class="site-footer" role="contentinfo">
            <div class="site-info">
                Powered by <a href="https://github.com/Sandra/Sandra.Snow" rel="generator">Sandra.Snow</a>.	
            </div>
            <!-- .site-info -->
        </footer>
        <!-- #colophon .site-footer -->
    </div>
    <!-- #page .hfeed .site -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src='/javascripts/prettify.js' type='text/javascript'></script>
    @Html.RenderGoogleAnalytics("UA-49651944-1")
    <script type='text/javascript'>
        $(function () {
            $("pre code").parent().each(function () {
                if (!$(this).hasClass("prettyprint")) {
                    $(this).addClass("prettyprint");
                    a = true
                }
            });

            prettyPrint();
        });
    </script>
    
    <!-- Authorship Google -->
    <a style="display: block; width: 30px; height: 30px; text-indent: -999px; text-decoration: none" href="https://plus.google.com/u/1/105055485720815974241?rel=author">Google</a>
</body>
</html>
